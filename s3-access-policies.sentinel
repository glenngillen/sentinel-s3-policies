import "tfplan"
check_policies = func() {
  pass = true
  for tfplan.resources.aws_s3_bucket as _, buckets {
    for buckets as _, bucket {
      print(bucket)
      if bucket.applied.acl is not "public-read" {
        pass = false
      }
    }
  }
  return pass
}
policies_validated = check_policies()
main = rule {
  policies_validated
}